{
  "name": "@agions/taroviz-charts",
  "version": "1.1.1",
  "description": "TaroViz 图表组件集合",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./LineChart": {
      "import": "./dist/components/LineChart.js",
      "require": "./dist/components/LineChart.js",
      "types": "./dist/LineChart/index.d.ts"
    },
    "./BarChart": {
      "import": "./dist/components/BarChart.js",
      "require": "./dist/components/BarChart.js",
      "types": "./dist/BarChart/index.d.ts"
    },
    "./PieChart": {
      "import": "./dist/components/PieChart.js",
      "require": "./dist/components/PieChart.js",
      "types": "./dist/PieChart/index.d.ts"
    },
    "./ScatterChart": {
      "import": "./dist/components/ScatterChart.js",
      "require": "./dist/components/ScatterChart.js",
      "types": "./dist/ScatterChart/index.d.ts"
    },
    "./RadarChart": {
      "import": "./dist/components/RadarChart.js",
      "require": "./dist/components/RadarChart.js",
      "types": "./dist/RadarChart/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "webpack --mode=development --watch",
    "build": "npx webpack --mode=production",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@agions/taroviz-adapters": "1.1.0",
    "@agions/taroviz-core": "1.1.0"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3.4.0",
    "@tarojs/taro": ">=3.4.0",
    "echarts": ">=5.4.0",
    "react": ">=16.13.0"
  },
  "gitHead": "08705385bf791a02d7eb7d74ffbf44f021f26b5b",
  "devDependencies": {
    "webpack-cli": "^6.0.1"
  }
}