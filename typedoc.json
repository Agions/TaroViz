{
  "entryPoints": [
    "packages/all/src/index.ts",
    "packages/core/src/index.ts",
    "packages/charts/src/index.ts",
    "packages/themes/src/index.ts",
    "packages/data/src/index.ts",
    "packages/hooks/src/index.ts",
    "packages/adapters/src/index.ts"
  ],
  "out": "docs-api",
  "exclude": ["**/__tests__/**", "**/node_modules/**"],
  "theme": "default",
  "name": "TaroViz API文档",
  "readme": "README.md",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Charts",
    "Themes",
    "Data Processing",
    "Utilities",
    "Types"
  ],
  "searchInComments": true,
  "sort": ["static-first", "visibility", "alphabetical"],
  "plugin": [],
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/agions/taroviz/blob/{gitRevision}/{path}#L{line}",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "sidebarLinks": {
    "使用指南": "https://github.com/agions/taroviz/blob/main/docs/USAGE.md",
    "GitHub仓库": "https://github.com/agions/taroviz"
  },
  "validation": {
    "invalidLink": true,
    "notDocumented": false
  }
} 