{
  "name": "taroviz",
  "version": "0.1.0",
  "description": "TaroViz 是一个多端兼容的 Apache ECharts 图表组件，专为 Taro 框架打造。支持在 H5、微信小程序、支付宝小程序和鸿蒙OS 等多个平台下无缝使用 ECharts 强大的可视化能力",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:h5": "TARO_ENV=h5 rollup -c",
    "build:weapp": "TARO_ENV=weapp rollup -c",
    "build:alipay": "TARO_ENV=alipay rollup -c",
    "build:harmony": "TARO_ENV=harmony rollup -c",
    "build:all": "npm run build:h5 && npm run build:weapp && npm run build:alipay && npm run build:harmony",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepare": "npm run build"
  },
  "keywords": [
    "taro",
    "echarts",
    "react",
    "chart",
    "visualization"
  ],
  "author": "Agions",
  "license": "MIT",
  "peerDependencies": {
    "@tarojs/components": ">=3.6.0",
    "@tarojs/plugin-framework-react": ">=3.6.0",
    "@tarojs/plugin-platform-alipay": ">=3.6.0",
    "@tarojs/plugin-platform-h5": ">=3.6.0",
    "@tarojs/plugin-platform-harmony": "3.6.0",
    "@tarojs/plugin-platform-weapp": ">=3.6.0",
    "@tarojs/react": ">=3.6.0",
    "@tarojs/runtime": ">=3.6.0",
    "@tarojs/taro": ">=3.6.0",
    "echarts": ">=5.4.0",
    "echarts-for-react": ">=3.0.2",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-runtime": "7.26.10",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/runtime-corejs3": "7.26.10",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^11.1.0",
    "@tarojs/cli": "3.6.15",
    "@tarojs/components": "3.6.15",
    "@tarojs/plugin-framework-react": "3.6.15",
    "@tarojs/plugin-platform-alipay": "3.6.15",
    "@tarojs/plugin-platform-h5": "3.6.15",
    "@tarojs/plugin-platform-harmony": "3.6.0",
    "@tarojs/plugin-platform-weapp": "3.6.15",
    "@tarojs/react": "3.6.15",
    "@tarojs/runtime": "3.6.15",
    "@tarojs/taro": "3.6.15",
    "@tarojs/webpack5-runner": "3.6.15",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "echarts": "5.6.0",
    "echarts-for-react": "3.0.3",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "node-sass": "9.0.0",
    "resize-detector": "0.3.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-scss": "4.0.1",
    "rollup-plugin-typescript2": "0.36.0",
    "sass": "^1.69.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@babel/runtime": "7.26.10"
  }
